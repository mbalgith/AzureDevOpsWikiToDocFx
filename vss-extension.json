{
    "manifestVersion": 1,
    "id": "AXADevOpsWikiToDocFx", 
    "name": "AXA DevOps Wiki to DocFx",
    "version": "0.3",
   "publisher": "IT-Habegger",
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "A task that generates a DocFX website out of your Azure DevOps wiki repository with the AXA design",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "icon.png"
    },
    "files": [
      {
        "path": "AXADevOpsWikiToDocFx"
      }
    ],
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "repository": {
      "type": "git",
      "uri": "https://github.com/mbalgith/AzureDevOpsWikiToDocFx"
    },    
    "contributions": [
        {
            "id": "AXADevOpsWikiToDocFx",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
          "properties": {
            "name": "AXADevOpsWikiToDocFx"
          }
        }
    ]
}